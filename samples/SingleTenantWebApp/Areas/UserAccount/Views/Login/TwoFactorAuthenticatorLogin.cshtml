@model BrockAllen.MembershipReboot.Mvc.Areas.UserAccount.Models.TwoFactorAuthInputModel
@{
    ViewBag.Title = "Two Factor Authenticator";
}

<h2>To signin enter the code generated by an Authenticator application</h2>
@using (Html.BeginForm("TwoFactorAuthenticatorLogin", "Login"))
{
    @Html.ValidationSummary()
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x=>x.ReturnUrl)
    <p>
        @Html.LabelFor(x=>x.Code)
        @Html.EditorFor(x=>x.Code)
    </p>
    <p>
        <button name="button">Sign In</button>
    </p>
}
